{
  "name": "icebreaker",
  "description": "Deus Ex ICE breaker web component",
  "private": true,
  "author": "Eirik Albrigtsen <analsandblaster@gmail.com>",
  "version": "0.1.1",
  "repository": {
    "type": "git",
    "url": "clux/icebreaker"
  },
  "keywords": [
    "polymer",
    "component"
  ],
  "scripts": {
    "build-js": "browserify -r tape > bundle.js",
    "build-css": "stylus -U icebreaker.stylus",
    "build-html": "vulcanize icebreaker.html --inline-css --exclude bower_components/polymer/polymer.html > icebreaker.build.html",
    "build": "npm run build-js && npm run build-css && npm run build-html",
    "watch-css": "catw icebreaker.stylus -c 'npm run build-css' -w",
    "watch": "npm run watch-css",
    "pretestling": "npm run build",
    "testling": "testling -x google-chrome"
  },
  "dependencies": {},
  "devDependencies": {
    "catw": "^1.0.1",
    "stylus": "^0.51.1",
    "tape": "^4.0.0",
    "vulcanize": "^1.8.1"
  },
  "testling": {
    "html": "test/icebreaker.test.html"
  },
  "bugs": {
    "url": "http://github.com/clux/icebreaker/issues"
  },
  "license": "MIT"
}
